{
  "id": "RTSControls",
  "title": "RTS Controls",
  "description": "This module will attempt to add RTS like controls to token movement using the Routinglib library.\n\n",
  "version": "#{VERSION}#",
  "library": "false",
  "manifestPlusVersion": "1.2.0",
  "compatibility": {
    "minimum": 11,
    "verified": 11,
    "maximum": 11
  },
  "authors": [
    {
      "name": "Ingram Blakelock",
      "url": "https://github.com/Garsondee/RTSControls",
      "discord": "garsondee"
    }
  ],
  "relationships": {
    "systems": [],
    "requires": [
      {
        "id": "routinglib",
        "type": "module",
        "manifest": "https://raw.githubusercontent.com/manuelVo/foundryvtt-routinglib/master/module.json"
      }
    ]
  },
  "esmodules": [
    "scripts/module.js"
  ],
  "scripts": [
    "scripts/lib/lib.js"
  ],
  "styles": [
    "styles/module.css"
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "languages/en.json"
    }
  ],
  "url": "#{URL}#",
  "manifest": "#{MANIFEST}#",
  "download": "#{DOWNLOAD}#",
  "license": "LICENSE",
  "readme": "README.md",
  "media": [
    {
      "type": "icon",
      "url": "https://avatars2.githubusercontent.com/u/71292812?s=400&u=ccdb4eeb7abf551ca8f314e5a9bfd0479a4d3d41&v=4"
    }
  ]
}